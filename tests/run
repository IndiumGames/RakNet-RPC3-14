#/bin/bash

# When this exits, exit all back ground process also.
trap 'kill $(jobs -p)' EXIT

# Create empty log file.
truncate -s 0 tests/bin/test.log
truncate -s 0 tests/bin/garbage.txt


./tests/bin/raknet-tests --client-count 20 --call-count 20 > tests/bin/test.log &

tail -f tests/bin/test.log
